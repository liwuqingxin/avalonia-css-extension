{
	"version": "1.0.0",
	"name": "acss",
	"scopeName": "source.acss",
	"patterns": [
		{
			"include": "#comment-block"
		},
		{
			"include": "#combinators"
		},
		{
			"include": "#text-mate-standard-name"
		},
		{
			"include": "#selectors"
		},
		{
			"include": "#setters"
		}
	],
	"repository": {
		"comment-block": {
			"begin": "/\\*",
			"beginCaptures": {
				"0": {
					"name": "punctuation.definition.comment.begin.acss"
				}
			},
			"end": "\\*/",
			"endCaptures": {
				"0": {
					"name": "punctuation.definition.comment.end.acss"
				}
			},
			"name": "comment.block.acss"
		},
		"combinators": {
			"patterns": [
				{
					"match": "/deep/|>>>",
					"name": "invalid.deprecated.combinator.acss"
				},
				{
					"match": ">|\\+|~|\\[\\[|\\]\\]",
					"name": "keyword.operator.combinator.acss"
				}
			]
		},
		"commas": {
			"match": ",",
			"name": "punctuation.separator.list.comma.acss"
		},
		"text-mate-standard-name":{
			"patterns": [
				{
					"match": "comment.line.double-slash.acss",
					"name": "comment.line.double-slash.acss"
				},
				{
					"match": "comment.line.double-dash.acss",
					"name": "comment.line.double-dash.acss"
				},
				{
					"match": "comment.line.number-sign.acss",
					"name": "comment.line.number-sign.acss"
				},
				{
					"match": "comment.line.percentage.acss",
					"name": "comment.line.percentage.acss"
				},
				{
					"match": "comment.block.acss",
					"name": "comment.block.acss"
				},
				{
					"match": "comment.block.documentation.acss",
					"name": "comment.block.documentation.acss"
				},
				{
					"match": "constant.acss",
					"name": "constant.acss"
				},
				{
					"match": "constant.numeric.acss",
					"name": "constant.numeric.acss"
				},
				{
					"match": "constant.character.acss",
					"name": "constant.character.acss"
				},
				{
					"match": "constant.language.acss",
					"name": "constant.language.acss"
				},
				{
					"match": "constant.other.acss",
					"name": "constant.other.acss"
				},
				{
					"match": "entity.acss",
					"name": "entity.acss"
				},
				{
					"match": "entity.name.function.acss",
					"name": "entity.name.function.acss"
				},
				{
					"match": "entity.name.type.acss",
					"name": "entity.name.type.acss"
				},
				{
					"match": "entity.name.tag.acss",
					"name": "entity.name.tag.acss"
				},
				{
					"match": "entity.name.section.acss",
					"name": "entity.name.section.acss"
				},
				{
					"match": "entity.other.inherited-class.acss",
					"name": "entity.other.inherited-class.acss"
				},
				{
					"match": "entity.other.attribute-name.acss",
					"name": "entity.other.attribute-name.acss"
				},
				{
					"match": "invalid.acss",
					"name": "invalid.acss"
				},
				{
					"match": "invalid.illegal.acss",
					"name": "invalid.illegal.acss"
				},
				{
					"match": "invalid.deprecated.acss",
					"name": "invalid.deprecated.acss"
				},
				{
					"match": "keyword.acss",
					"name": "keyword.acss"
				},
				{
					"match": "keyword.control.acss",
					"name": "keyword.control.acss"
				},
				{
					"match": "keyword.operator.acss",
					"name": "keyword.operator.acss"
				},
				{
					"match": "keyword.other.acss",
					"name": "keyword.other.acss"
				},
				{
					"match": "markup.acss",
					"name": "markup.acss"
				},
				{
					"match": "markup.underline.acss",
					"name": "markup.underline.acss"
				},
				{
					"match": "markup.underline.link.acss",
					"name": "markup.underline.link.acss"
				},
				{
					"match": "markup.bold.acss",
					"name": "markup.bold.acss"
				},
				{
					"match": "markup.heading.acss",
					"name": "markup.heading.acss"
				},
				{
					"match": "markup.italic.acss",
					"name": "markup.italic.acss"
				},
				{
					"match": "markup.list.acss",
					"name": "markup.list.acss"
				},
				{
					"match": "markup.list.numbered.acss",
					"name": "markup.list.numbered.acss"
				},
				{
					"match": "markup.list.unnumbered.acss",
					"name": "markup.list.unnumbered.acss"
				},
				{
					"match": "markup.quote.acss",
					"name": "markup.quote.acss"
				},
				{
					"match": "markup.raw.acss",
					"name": "markup.raw.acss"
				},
				{
					"match": "markup.other.acss",
					"name": "markup.other.acss"
				},
				{
					"match": "meta.acss",
					"name": "meta.acss"
				},
				{
					"match": "meta.function.acss",
					"name": "meta.function.acss"
				},
				{
					"match": "meta.storage.type.acss",
					"name": "meta.storage.type.acss"
				},
				{
					"match": "meta.entity.name.function.acss",
					"name": "meta.entity.name.function.acss"
				},
				{
					"match": "meta.variable.parameter.acss",
					"name": "meta.variable.parameter.acss"
				},
				{
					"match": "storage.type.acss",
					"name": "storage.type.acss"
				},
				{
					"match": "storage.modifier.acss",
					"name": "storage.modifier.acss"
				},
				{
					"match": "string.quoted.acss",
					"name": "string.quoted.acss"
				},
				{
					"match": "string.quoted.single.acss",
					"name": "string.quoted.single.acss"
				},
				{
					"match": "string.quoted.double.acss",
					"name": "string.quoted.double.acss"
				},
				{
					"match": "string.quoted.triple.acss",
					"name": "string.quoted.triple.acss"
				},
				{
					"match": "string.quoted.other.acss",
					"name": "string.quoted.other.acss"
				},
				{
					"match": "string.unquoted.acss",
					"name": "string.unquoted.acss"
				},
				{
					"match": "string.interpolated.acss",
					"name": "string.interpolated.acss"
				},
				{
					"match": "string.regexp.acss",
					"name": "string.regexp.acss"
				},
				{
					"match": "string.other.acss",
					"name": "string.other.acss"
				},
				{
					"match": "support.acss",
					"name": "support.acss"
				},
				{
					"match": "support.function.acss",
					"name": "support.function.acss"
				},
				{
					"match": "support.class.acss",
					"name": "support.class.acss"
				},
				{
					"match": "support.type.acss",
					"name": "support.type.acss"
				},
				{
					"match": "support.constant.acss",
					"name": "support.constant.acss"
				},
				{
					"match": "support.variable.acss",
					"name": "support.variable.acss"
				},
				{
					"match": "support.other.acss",
					"name": "support.other.acss"
				},
				{
					"match": "variable.acss",
					"name": "variable.acss"
				},
				{
					"match": "variable.parameter.acss",
					"name": "variable.parameter.acss"
				},
				{
					"match": "variable.language.acss",
					"name": "variable.language.acss"
				},
				{
					"match": "variable.other.acss",
					"name": "variable.other.acss"
				}
			]
		},
		"selectors": {
			"patterns": [
				{
					"match": "\\[([a-zA-Z0-9_]*?)=([a-zA-Z0-9_]*?)\\]",
					"captures": {
						"1": {
							"name": "support.class.property.acss"
						},
						"2": {
							"name": "markup.raw.value.acss"
						}
					},
					"name": "meta.selector.acss"
				},
				{
					"match": "\\[\\(([a-zA-Z0-9_]*?)\\.([a-zA-Z0-9_]*?)\\)=([a-zA-Z0-9_]*?)\\]",
					"captures": {
						"1": {
							"name": "support.class.acss"
						},
						"2": {
							"name": "support.class.property.acss"
						},
						"3": {
							"name": "markup.raw.value.acss"
						}
					},
					"name": "meta.selector.acss"
				},
				{
					"match":"[\\.:][a-zA-Z0-9_-]*",
					"name":"meta.selector.acss"
				},
				{
					"match":"/template/",
					"name":"keyword.other.selector.template.acss"
				},
				{
					"match":"\\#[a-zA-Z0-9_-]*",
					"name":"entity.name.section.selector.acss"
				}
			]
		},
		"setters": {
			"begin": "\\{",
			"end": "\\}",
			"name": "meta.setters.acss",
			"patterns": [
				{
					"include": "#selectors"
				},
				{
					"match": "([a-zA-Z0-9_]*?):(.*?)[;$]",
					"captures": {
						"1": {
							"name": "support.class.property.acss"
						},
						"2": {
							"name": "markup.raw.value.acss"
						}
					},
					"name": "meta.selector.acss"
				}
			]
		}
	}
}